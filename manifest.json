{
	"manifest_version": 2,
	"name": "Meduzent",
	"version": "1.0.0",
	"description": "Remove foreign agent mark from meduza.io.",
	"short_name": "MZNT",
	"permissions": ["https://meduza.io/*"],
	"content_scripts": [
		{
			"matches": ["https://meduza.io/*"],
			"css": ["meduzent.css"]
		}
	],
	"icons": {
        "32": "icon32.png",
        "128": "icon128.png"
    }
}